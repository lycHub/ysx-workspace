{
  "name": "@dci/shared",
  "version": "0.0.0",
  "private": true,
  "typings": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "pnpm run build:sass",
    "build:sass": "sass --no-source-map --load-path=node_modules/modern-normalize src/styles/index.scss:src/styles/index.css"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0"
  },
  "devDependencies": {
    "sass": "^1.58.3"
  }
}
